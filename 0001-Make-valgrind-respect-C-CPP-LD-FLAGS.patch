---
 mpi/Makefile.am | 3 ---
 1 file changed, 3 deletions(-)

diff --git a/mpi/Makefile.am b/mpi/Makefile.am
index 054b37b75..1e99689ee 100644
--- a/mpi/Makefile.am
+++ b/mpi/Makefile.am
@@ -7,9 +7,6 @@ include $(top_srcdir)/Makefile.all.am
 CC = $(MPI_CC)
 DEFS =
 DEFAULT_INCLUDES =
-CPPFLAGS =
-CFLAGS =
-LDFLAGS =
 
 EXTRA_DIST = \
 	mpiwrap_type_test.c
